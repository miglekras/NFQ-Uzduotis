CREATE TABLE specialist(
    id INT(10) NOT NULL PRIMARY KEY AUTO_INCREMENT,
    title varchar(30) not null,
      updated_at TIMESTAMP NOT NULL DEFAULT NOW() ON UPDATE NOW(),
      created_at TIMESTAMP NOT NULL
);

