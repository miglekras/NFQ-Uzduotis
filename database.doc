CREATE TABLE clients(
    id INT(10) NOT NULL PRIMARY KEY AUTO_INCREMENT,
    firstname varchar(30) not null,
    lastname varchar(60),
    specialist INT(10) NOT NULL,  
    CONSTRAINT fk_specialist FOREIGN KEY (specialist)
    REFERENCES specialist(id)
);
